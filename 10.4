#include <iostream>

using namespace std;

template <class T>
class stack {
    T st[100];
    int top;
public:
    stack() {
        top = -1;
    }
    
    void push(T value) {
        if (top >= 99) {
            cout << "Overflow" << endl;
        } else {
            top++;
            st[top] = value;
            cout << value << " pushed into stack" << endl;
        }
    }
    
    void pop() {
        if (top == -1) {
            cout << "Underflow" << endl;
        } else {
            cout << st[top] << " popped from stack" << endl;
            top--;
        }
    }
    
    void display() {
        if (top == -1) {
            cout << "Stack is empty" << endl;
        } else {
            cout << "Stack elements: ";
            for (int i = top; i >= 0; i--) {
                cout << st[i] << " ";
            }
            cout << endl;
        }
    }
};

int main() {
    stack<int> s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.display();
    s.pop();
    s.display();
    s.pop();
    s.pop();
    s.pop(); 
    
    return 0;
}